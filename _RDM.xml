			<equip set="healingset"/>
	  </if>
     <if ad_name="Refresh*"><equip><body>Atrophy Tabard +3</body></equip></if>
   </midmagic>
   <idlegear>
      <if advanced="$init=0">
            <setvar name="init" value="1" />
            <addtochat color="8">Staring up WHM</addtochat>
            <command delay="1000">/macro book 13</command>
            <command delay="1500">/macro set 1</command>
            <command delay="2000">/lockstyleset 96</command>                 
        </if>
      <if p_status="engaged">
            <equip set="TP"/>
         <if advanced="$DT=On">
            <equip set="DT" />
         </if>
         <elseif>
            <if advanced="$kite=On">
               <equip set="Move" />
            </if>
         </elseif>
         <else>
            <addtochat color="8">TP Set</addtochat>   
            <equip set="TP"/>
         </else>
      </if>
      <elseif p_status="idle">
