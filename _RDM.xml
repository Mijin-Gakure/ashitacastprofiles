   <inputcommands>
      <cmd input="/Warp">
         <equip>
            <lring>Warp Ring</lring>
         </equip>
         <equip>
            <rring>Dim. Ring (Dem)</rring>
         </equip>
      </cmd>
      <cmd input="/thawk">
         <equip>
            <ammo>Tomahawk</ammo>
         </equip>
      </cmd>
      <cmd input="/DT">
         <if advanced="$DT=Off">
            <setvar name="DT" value="On" />
            <addtochat color="8">DT Mode: [Enabled]</addtochat>
            <doidlegear />
         </if>
         <else>
            <setvar name="DT" value="Off" />
            <addtochat color="8">DT Mode: [Disabled]</addtochat>
            <doidlegear />
         </else>
      </cmd>
      <cmd input="/kite">
         <if advanced="$kite=Off">
            <setvar name="kite" value="On" />
            <addtochat color="8">Kite Mode: [Enabled]</addtochat>
            <doidlegear />
         </if>
         <else>
            <setvar name="kite" value="Off" />
            <addtochat color="8">Kite Mode: [Disabled]</addtochat>
            <doidlegear />
         </else>
      </cmd>
      <cmd input="/acc">
         <if advanced="$acc=Off">
            <setvar name="ACC" value="On" />
            <addtochat color="8">ACC Mode: [Enabled]</addtochat>
            <doidlegear />
         </if>
         <else>
            <setvar name="ACC" value="Off" />
            <addtochat color="8">ACC Mode: [Disabled]</addtochat>
            <doidlegear />
         </else>
      </cmd>
   </inputcommands>
   <jobability>
      <if ad_name="Composure">
