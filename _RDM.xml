      <if ad_name="stoneskin|Cure*"><equip><legs>Doyen Pants</legs></equip></if>
   </premagic>
   <midmagic>
      <if ad_skill="DarkMagic">
         <if ad_name="Drain*|Aspir*">
            <equip set="Drain" />
         </if>
         <elseif ad_name="Absorb-*">
            <equip set="Absorb" />
         </elseif>
         <else>
            <equip set="Dark" />
         </else>
         <elseif ad_name="Dread*">
            <equip set="Dread Spikes" />
         </elseif>
      </if>
      <if ad_skill="EnfeeblingMagic">
