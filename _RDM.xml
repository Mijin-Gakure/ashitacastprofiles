            <addtochat color="8">Equipping Potency Enfeebling Set</addtochat>
            <equip set="Potency"/>
         </if>
         <else>
            <addtochat color="8">Equipping MACC Enfeebling Set</addtochat>
            <equip set="MACC" />
         </else>
      </if>
      <if ad_skill="ElementalMagic">
            <addtochat color="8">Elemental Set</addtochat>	  
         <if ad_name="Impact">
            <equip set="Impact" />
         </if>
         <else>
            <equip set="Elemental" />
         </else>
      </if>
	  <if ad_skill="EnhancingMagic">
