<inputcommands>
        <cmd input="/Warp">
            <equip><lring>Warp Ring</lring></equip>
            <equip><rring>Dim. Ring (Dem)</rring></equip>
        </cmd>
        <cmd input="/clear">
            <equip set="DT"/>
                <setvar name="mdt" value="Off" />
                <setvar name="DT" value="Off" />
                <setvar name="hdt" value="Off" />
                <addtochat color="8">------------[Clearing all variables]--------------</addtochat>
        </cmd>
        <cmd input="/DT">   <!-- Set a keybind to /DT to toggle easily -->
            <setvar name="hdt" value="Off"/>
            <setvar name="mdt" value="Off"/>
            <setvar name="dd" value="Off"/>
            <if advanced="$DT=Off">
                <setvar name="DT" value="On" />
                <addtochat color="8">DT Mode: [Enabled]</addtochat>
                <doidlegear />
            </if>
            <else>
                <setvar name="DT" value="Off" />
                <addtochat color="8">DT Mode: [Disabled]</addtochat>
                <doidlegear />
            </else>
        </cmd>
        <cmd input="/MDT">   <!-- Set a keybind to /DT to toggle easily -->
            <setvar name="DT" value="Off"/>
            <setvar name="hdt" value="Off"/>
            <setvar name="dd" value="Off"/>
            <if advanced="$mdt=Off">
                <setvar name="mdt" value="On" />
                <addtochat color="8">MDT/MEVA Mode: [Enabled]</addtochat>
                <doidlegear />
            </if>
            <else>
                <setvar name="mdt" value="Off" />
                <addtochat color="8">MDT/MEVA Mode: [Disabled]</addtochat>
                <doidlegear />
            </else>
        </cmd>
        <cmd input="/hdt">   <!-- Set a keybind to /DT to toggle easily -->
            <setvar name="DT" value="Off"/>
            <setvar name="mdt" value="Off"/>
            <setvar name="dd" value="Off"/>
            <if advanced="$hdt=Off">
                <setvar name="hdt" value="On" />
                <addtochat color="8">HDT Mode: [Enabled]</addtochat>
                <doidlegear />
            </if>
            <else>
                <setvar name="hdt" value="Off" />
                <addtochat color="8">HDT Mode: [Disabled]</addtochat>
                <doidlegear />
            </else>
        </cmd>

        <cmd input="/kite">    <!-- Set a keybind to /kite to toggle easily -->
            <if advanced="$kite=Off">
                <setvar name="kite" value="On" />
                <addtochat color="8">Kite Mode: [Enabled]</addtochat>
                <doidlegear />
            </if>
            <else>
                <setvar name="kite" value="Off" />
                <addtochat color="8">Kite Mode: [Disabled]</addtochat>
                <doidlegear />
