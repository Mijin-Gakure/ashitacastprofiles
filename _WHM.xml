   <premagic>
      <equip set="Precast" />
      <if ad_type="whitemagic">
         <if ad_skill="healingmagic">
            <!--<addtochat color="36">Pre Healing Magic</addtochat>-->
            <if ad_name="Cure*|Cura*">
               <equip set="PrecastCure" />
            </if>
            <elseif ad_name="*na">
                  <addtochat color="36">Pre Healing Magic</addtochat>

               <equip set="PrecastGeneral" />
               <equip>
                  <main>Yagrush</main>
                  <legs>Ebers Pant. +1</legs>
               </equip>
            </elseif>
            <else>
               <equip set="PrecastGeneral" />
            </else>
         </if>
         <elseif ad_skill="divinemagic">
            <!--<addtochat color="36">Pre Divine Magic</addtochat>-->
         </elseif>
         <elseif ad_skill="enhancingmagic">
            <equip set="PrecastGeneral" />

            <if ad_name="Erase">
               <equip>
                  <main>Yagrush</main>
                  <legs>Ebers Pant. +1</legs>
               </equip>               
            </if>
            <!--<addtochat color="36">Pre White Enhancing Magic</addtochat>-->
         </elseif>
         <elseif ad_skill="enfeeblingmagic">
            <!--<addtochat color="36">Pre White Enfeebling Magic</addtochat>-->
            <equip set="PrecastGeneral" />
         </elseif>
         <else>
            <equip set="PrecastGeneral" />
         </else>
      </if>
      <elseif ad_type="blackmagic">
         <if ad_skill="elementalmagic">
            <equip set="PrecastGeneral" />
            <!--<addtochat color="36">Pre Black Elemental Magic</addtochat>-->
         </if>
         <elseif ad_skill="darkmagic">
            <!--<addtochat color="36">Pre Black Dark Magic</addtochat>-->
            <equip set="PrecastGeneral" />
         </elseif>
         <elseif ad_skill="enhancingmagic">
            <equip set="EnhPre" />
            <!--<addtochat color="36">Pre Black Enhancing Magic</addtochat>-->
            <equip set="PrecastGeneral" />
         </elseif>
         <elseif ad_skill="enfeeblingmagic">
            <!--<addtochat color="36">Pre Black Enfeebling Magic</addtochat>-->
            <equip set="PrecastGeneral" />
         </elseif>
      </elseif>
   </premagic>
   <midmagic>
      <if ad_type="whitemagic">
         <!--<addtochat color="36">Mid White Magic</addtochat>-->
         <if ad_skill="healingmagic">
            <!--<addtochat color="36">Mid Healing Magic</addtochat>-->
            <if ad_name="Cure*|Cura|Curaga*">
               <equip set="MidcastCure" />
            </if>
            <elseif ad_name="*na">
               <if ad_name="Cursna">
                  <equip set="MidcastCursna" />
               </if>
               <else>
                  <equip set="MidcastStatusCure" />
               </else>
            </elseif>
         </if>
         <elseif ad_skill="divinemagic">
            <!--<addtochat color="36">Mid Divine Magic</addtochat>-->
            <equip set="MidcastDivine" />
         </elseif>
         <elseif ad_skill="enhancingmagic">
            <addtochat color="36">Mid White Enhancing Magic</addtochat>
            <if ad_name="Refresh">
               <equip set="SelfRefresh" />
            </if>
            <elseif ad_name="Bar*">
               <equip set="MidcastBarspell" />
            </elseif>
            <elseif ad_name="Regen*">
               <equip set="MidcastRegen" />
            </elseif>
            <elseif ad_name="Stoneskin">
               <!--<addtochat color="36">Mid Stoneskin Magic</addtochat>-->
               <equip set="MidcastEnhancing" />
            </elseif>
            <elseif ad_name="Erase">
            <addtochat color="36">Erase</addtochat>

               <equip set="MidcastEnhancing" />
               <equip><main>yagrush</main></equip>
            </elseif>
            <else>
               <equip set="MidcastEnhancing" />
            </else>
         </elseif>
         <elseif ad_skill="enfeeblingmagic">
            <!--<addtochat color="36">Mid White Enfeebling Magic</addtochat>-->
            <equip set="Enfeebles" />
         </elseif>
      </if>
      <elseif ad_type="blackmagic">
         <!--<addtochat color="36">Mid Black Magic</addtochat>-->
         <if ad_skill="elementalmagic">
            <!--<addtochat color="36">Mid Black Elemental Magic</addtochat>-->
            <if ad_name="*helix">
               <equip set="Helix" />
            </if>
            <if advanced="$Burst=1">
               <equip set="MB" />
            </if>
            <elseif>
               <equip set="Nukes" />
            </elseif>
         </if>
         <elseif ad_skill="darkmagic">
            <!--<addtochat color="36">Mid Black Dark Magic</addtochat>-->
            <if ad_name="Drain|Aspir*">
               <equip set="DarkMagic" />
            </if>
            <elseif ad_name="Death">
               <equip set="Death" />
            </elseif>
            <elseif>
               <equip set="Enfeebles" />
            </elseif>
         </elseif>
         <elseif ad_skill="enhancingmagic">
            <!--<addtochat color="36">Mid Black Enhancing Magic</addtochat>-->
            <if ad_name="Stoneskin">
               <equip set="SS" />
            </if>
            <else>
               <equip set="Enhancing" />
            </else>
         </elseif>
         <elseif ad_skill="enfeeblingmagic">
            <!--<addtochat color="36">Mid Black Enfeebling Magic</addtochat>-->
            <equip set="Enfeebles" />
         </elseif>
         <elseif ad_name="Impact">
            <equip set="Impact" />
         </elseif>
      </elseif>
   </midmagic>
